body\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col\">\r\n                      <h5 class=\"card-title text-uppercase text-muted mb-0\">OUT</h5>\r\n                      <span class=\"h2 font-weight-bold mb-0\">{{uout}}</span>\r\n                    </div>\r\n                    <div class=\"col-auto\">\r\n                      <div class=\"icon icon-shape bg-gradient-green text-white rounded-circle shadow\">\r\n                        <i class=\"ni ni-money-coins\"></i>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-xl-3 col-md-6\">\r\n              <div data-aos=\"zoom-in\" data-aos-delay=\"200\" class=\"card card-stats\">\r\n                <!-- Card body -->\r\n                <div class=\"card-body\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col\">\r\n                      <h5 class=\"card-title text-uppercase text-muted mb-0\">Напруга</h5>\r\n                      <span class=\"h2 font-weight-bold mb-0\">{{volt}}</span>\r\n                    </div>\r\n                    <div class=\"col-auto\">\r\n                      <div class=\"icon icon-shape bg-gradient-info text-white rounded-circle shadow\">\r\n                        <i class=\"ni ni-chart-bar-32\"></i>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <p v-if=\"difv>=0\" class=\"mt-3 mb-0 text-sm\">\r\n                    <span class=\"text-success mr-2\"><i class=\"fa fa-arrow-up\"></i>{{difv}}%</span>\r\n                  </p>\r\n                  <p v-else class=\"mt-3 mb-0 text-sm\">\r\n                    <span class=\"text-warning mr-2\"><i class=\"fa fa-arrow-down\"></i>{{-1*difv}}%</span>\r\n                  </p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- Page content -->\r\n    <div class=\"container-fluid mt--6\">\r\n      <div class=\"row\">\r\n        <div class=\"col-xl-8\">\r\n          <div class=\"card bg-default\">\r\n            <div class=\"card-header bg-transparent\">\r\n              <div class=\"row align-items-center\">\r\n                <div class=\"col\">\r\n                  <h6 class=\"text-light text-uppercase ls-1 mb-1\">Огляд</h6>\r\n                  <h5 class=\"h3 text-white mb-0\">Графіки</h5>\r\n                </div>\r\n                <div class=\"col\">\r\n\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div data-aos=\"slide-right\" data-aos-delay=\"200\" class=\"card-body\">\r\n              <highcharts :options=\"chartOptionst\"></highcharts>\r\n            </div>\r\n             <div data-aos=\"slide-left\" data-aos-delay=\"200\" class=\"card-body\">\r\n              <highcharts :options=\"chartOptionsv\"></highcharts>\r\n            </div>\r\n             <div data-aos=\"slide-right\" data-aos-delay=\"200\" class=\"card-body\">\r\n              <highcharts :options=\"chartOptionstou\"></highcharts>\r\n            </div>\r\n             <div data-aos=\"slide-left\" data-aos-delay=\"200\" class=\"card-body\">\r\n              <highcharts :options=\"chartOptionstoi\"></highcharts>\r\n            </div>\r\n\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"row\">\r\n        <div class=\"col-xl-8\">\r\n          <div data-aos=\"fade-in\" data-aos-delay=\"200\" class=\"card\">\r\n            <div class=\"card-header border-0\">\r\n              <div class=\"row align-items-center\">\r\n                <div class=\"col\">\r\n                  <h3 class=\"mb-0\">Статистика</h3>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"table-responsive\">\r\n              <!-- Projects table -->\r\n              <table class=\"table align-items-center table-flush\">\r\n                <thead class=\"thead-light\">\r\n                  <tr>\r\n                    <th scope=\"col\">Параметр</th>\r\n                    <th scope=\"col\">Старе значення</th>\r\n                    <th scope=\"col\">Нове значення</th>\r\n                    <th scope=\"col\">Зміна</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr>\r\n                    <th scope=\"row\">\r\n                      Температура\r\n                    </th>\r\n                    <td>\r\n                      {{tempo}}\r\n                    </td>\r\n                    <td>\r\n                      {{temp}}\r\n                    </td>\r\n                    <td v-if=\"dift>0\">\r\n                      <i  class=\"fas fa-arrow-up text-success mr-3\"></i> {{dift}}%\r\n                    </td>\r\n                    <td v-else>\r\n                      <i  class=\"fas fa-arrow-down text-warning mr-3\"></i> {{-1*dift}}%\r\n                    </td>\r\n                  </tr>\r\n\r\n                  <tr>\r\n                    <th scope=\"row\">\r\n                      UIN\r\n                    </th>\r\n                    <td>\r\n                      {{uino}}\r\n                    </td>\r\n                    <td>\r\n                      {{uin}}\r\n                    </td>\r\n                    <td v-if=\"uin>uino\">\r\n                      <i class=\"fas fa-arrow-down text-warning mr-3\"></i> +1\r\n                    </td>\r\n                    <td v-else-if=\"uin<uino\">\r\n                      <i class=\"fas fa-arrow-down text-warning mr-3\"></i> -1\r\n                    </td>\r\n                    <td v-else>\r\n                      &emsp;&emsp;&ensp; 0\r\n                    </td>\r\n                  </tr>\r\n                  <tr>\r\n                    <th scope=\"row\">\r\n                      UOUT\r\n                    </th>\r\n                    <td>\r\n                      {{uouto}}\r\n                    </td>\r\n                    <td>\r\n                      {{uout}}\r\n                    </td>\r\n                    <td v-if=\"uout>uouto\">\r\n                      <i class=\"fas fa-arrow-down text-warning mr-3\"></i> +1\r\n                    </td>\r\n                    <td v-else-if=\"uout<uouto\">\r\n                      <i class=\"fas fa-arrow-down text-warning mr-3\"></i> -1\r\n                    </td>\r\n                    <td v-else>\r\n                      &emsp;&emsp;&ensp; 0\r\n                    </td>\r\n                  </tr>\r\n                  <tr>\r\n                    <th scope=\"row\">\r\n                      Напруга\r\n                    </th>\r\n                    <td>\r\n                      {{volto}}\r\n                    </td>\r\n                    <td>\r\n                      {{volt}}\r\n                    </td>\r\n                    <td v-if=\"difv>0\">\r\n                      <i  class=\"fas fa-arrow-up text-success mr-3\"></i> {{difv}}%\r\n                    </td>\r\n                    <td v-else>\r\n                      <i  class=\"fas fa-arrow-down text-warning mr-3\"></i> {{-1*difv}}%\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </div>\r\n <vue-final-modal v-model=\"show\" :drag=\"true\" :resize=\"true\" :keep-changed-style=\"true\" :esc-to-close=\"true\"\r\n classes=\"modal-container\" content-class=\"modal-content\">\r\n    </vue-final-modal>\r\n      </div></div></div></div>\r\n  <footer class=\"py-5\" id=\"footer-main\">\r\n    <div class=\"container\">\r\n      <div class=\"row align-items-center justify-content-xl-between\">\r\n        <div class=\"col-xl-6\">\r\n          <div class=\"copyright text-center text-xl-left text-muted\">\r\n            &copy; 2023 <a href=\"https://github.com/elaiviaien\" class=\"font-weight-bold ml-1\" target=\"_blank\">elaiviaien</a>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-xl-6\">\r\n          <ul class=\"nav nav-footer justify-content-center justify-content-xl-end\">\r\n            <li class=\"nav-item\">\r\n              <a  class=\"nav-link\" target=\"_blank\">elaiviaien</a>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <a href=\"https://t.me/liumphis\" class=\"nav-link\" target=\"_blank\">Співпраця</a>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <a href=\"http://hiker.elaiviaien.com\" class=\"nav-link\" target=\"_blank\">Сайт для туризму</a>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </footer>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport {Chart} from 'highcharts-vue'\r\nimport axios from \"axios\";\r\nimport {useToast} from \"vue-toastification\";\r\n\r\nexport default {\r\n  name: 'obj_page',\r\n  props:[\"slug\"],\r\n  components: {\r\n    highcharts:Chart\r\n\r\n  },\r\n   data:()=>(\r\n           {stats:[],\r\n             temp:\"\",\r\n             tempo:\"\",\r\n             dift:\"\",\r\n             uin:\"\",\r\n             uino:\"\",\r\n             uout:\"\",\r\n             uouto:\"\",\r\n             volt:\"\",\r\n             volto:\"\",\r\n             difv:\"\",\r\n               chartOptionst: {},\r\n               chartOptionsv: {},\r\n               chartOptionstou: {},\r\n               chartOptionstoi: {},\r\n             period:{day:true, h6:false, h3:false, h:false, m30:false, m15:false, m5:false, m:false},\r\n             show:false,\r\n             \t\t\ttoast : useToast(),\r\n             loading : false,\r\n\r\n           }),\r\n  created(){\r\nthis.loadStats()\r\n    this.ready()\r\n  },\r\n  methods:{\r\n    loadStats(){\r\n      var that = this;\r\n       axios({url: this.$store.state.backendUrl+`api/v1/